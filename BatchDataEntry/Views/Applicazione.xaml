<Window x:Class="BatchDataEntry.Views.Applicazione"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchDataEntry.Views"
        mc:Ignorable="d"
        Title="Applicazione" Height="350" Width="400">
    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="252" Margin="10,10,0,0" VerticalAlignment="Top" Width="150"/>
        <Button x:Name="button" Content="Modifica" HorizontalAlignment="Left" Margin="62,267,0,0" VerticalAlignment="Top" Width="45" FontSize="10"/>
        <Button x:Name="button_Copy" Content="Nuovo" HorizontalAlignment="Left" Margin="10,267,0,0" VerticalAlignment="Top" Width="45" FontSize="10"/>
        <Button x:Name="button_Copy1" Content="Elimina" HorizontalAlignment="Left" Margin="115,267,0,0" VerticalAlignment="Top" Width="45" FontSize="10"/>
        <Label x:Name="label" HorizontalAlignment="Left" Margin="175,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy3" HorizontalAlignment="Left" Margin="179,189,0,0" VerticalAlignment="Top"/>
        <StackPanel HorizontalAlignment="Left" Height="274" Margin="175,10,0,0" VerticalAlignment="Top" Width="207">
            <Label x:Name="label1">
                <Label x:Name="label2" Content="Nome" FontSize="11"/>
            </Label>
            <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBoxNome" FontSize="11" Margin="10,0"/>
            <Label x:Name="label_Copy" HorizontalAlignment="Left" VerticalAlignment="Top">
                <Label x:Name="label_Copy2" Content="Tipo" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="11"/>
            </Label>
            <ComboBox x:Name="comboBox" FontSize="11" Margin="10,0"/>
            <CheckBox x:Name="checkBox" Content="Includi Csv" Margin="10,10,10,0" FontSize="11"/>
            <WrapPanel Height="24" Margin="10,10,10,0">
                <TextBox x:Name="textBox1" Height="23" TextWrapping="Wrap" Text="TextBoxSelectFile" Width="160" Margin="0,0,5,0" FontSize="11"/>
                <Button x:Name="button1" Content="..." Width="22"/>
            </WrapPanel>
        </StackPanel>
        <Button x:Name="button2" Content="Salva" HorizontalAlignment="Left" Margin="307,289,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button2_Copy" Content="Cancella" HorizontalAlignment="Left" Margin="227,289,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
