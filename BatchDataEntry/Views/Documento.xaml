<Window x:Class="BatchDataEntry.Views.Documento"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchDataEntry.Views"
        mc:Ignorable="d"
        Title="Documento" Height="600" Width="500">
    <Grid>
        <DockPanel LastChildFill="True">
            <DockPanel LastChildFill="True" DockPanel.Dock="Top" Height="526">
                <WrapPanel DockPanel.Dock="Left" HorizontalAlignment="Left" Width="284" Margin="0,0,0,10">
                    <WindowsFormsHost Width="274" Height="505" Panel.ZIndex="2" Margin="5,10,5,0"/>
                </WrapPanel>
                <StackPanel Background="#FFF1F1F1" Width="200" DockPanel.Dock="Right" HorizontalAlignment="Right" Margin="5,10">
                </StackPanel>
            </DockPanel>
            <StackPanel Height="30" DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right">
                <Button x:Name="button" Content="Prossimo" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="5,6" FontSize="11" Width="52"/>
                <Button x:Name="button1" Content="Salta" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="5,6" FontSize="11" Width="31"/>
                <Button x:Name="button1_Copy" Content="Interrompi" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="5,6" FontSize="11" Width="60"/>
            </StackPanel>
        </DockPanel>
    </Grid>
</Window>
